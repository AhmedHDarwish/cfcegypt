<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_invoice_eighty_commission_tree" model="ir.ui.view">
            <field name="name">account.invoice.eighty.commission.tree</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_invoice_tree"/>
            <field name="mode">primary</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="attributes">
                    <attribute name="string">Name(80)</attribute>
                </xpath>
                <xpath expr="//field[@name='name']" position="after">
                    <field name="invoice_partner_display_name" string="Customer"/>
                    <field name="invoice_date" string="Invoice Date"/>
                </xpath>
                <xpath expr="//field[@name='invoice_origin']" position="after">
                    <field name="team_id" />
                    <field name="team_leader" />
                    <field name="invoice_user_id" />
                </xpath>
                <xpath expr="//field[@name='amount_untaxed_signed']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='state']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='invoice_date_due']" position="after">
                    <field name="paid_amount" sum="Amount"/>
                    <field name="sh_paid_amount_for_commision"/>
                    <field name="team_leader_eighty_percent" string="Leader %"/>
                    <field name="team_leader_eighty_percent_commission_value" string="Leader Commission" sum="Amount"/>
<!--                    <field name="sale_person_eighty_percent" string="Sales Person %"/>-->
<!--                    <field name="sale_person_eighty_percent_commission_value" string="Person Commission" />-->
                </xpath>
            </field>
        </record>

